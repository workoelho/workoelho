    "iconv-lite": "^0.6.3",
    "mime": "^4.0.1",
    "npmlog": "^7.0.1",
    "parse-multipart-data": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "send": "^0.18.0",
    "superstruct": "^1.0.3",
    "tsx": "^4.7.0",
    "typescript": "~5.3.3",
    "urlpattern-polyfill": "^10.0.0"
  },
  "devDependencies": {
    "@tsconfig/vite-react": "^3.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/content-type": "^1.1.8",
